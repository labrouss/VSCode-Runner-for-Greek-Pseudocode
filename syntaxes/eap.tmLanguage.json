{
  "scopeName": "source.eap",
  "patterns": [
    {
      "name": "comment.line.eap",
      "match": "//.*$"
    },
    {
      "name": "comment.block.eap",
      "begin": "/\\*",
      "end": "\\*/"
    },
    {
      "name": "string.quoted.double.eap",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.eap",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "keyword.control.eap",
      "match": "\\b(ΑΛΓΟΡΙΘΜΟΣ|ΣΤΑΘΕΡΕΣ|ΔΕΔΟΜΕΝΑ|ΑΡΧΗ|ΤΕΛΟΣ|ΕΑΝ|ΤΟΤΕ|ΑΛΛΙΩΣ|ΕΑΝ-ΤΕΛΟΣ|ΓΙΑ|ΑΠΟ|ΕΩΣ|ΜΕ|ΒΗΜΑ|ΜΕ-ΒΗΜΑ|ΕΠΑΝΑΛΑΒΕ|ΓΙΑ-ΤΕΛΟΣ|ΕΝΟΣΩ|ΕΝΟΣΩ-ΤΕΛΟΣ|ΜΕΧΡΙ|PRINT|READ|FUNCTION|PROCEDURE|BEGIN|END)\\b"
    },
    {
      "name": "keyword.other.commands.eap",
      "match": "\\b(ΤΥΠΩΣΕ|ΔΙΑΒΑΣΕ|ΥΠΟΛΟΓΙΣΕ)\\b"
    },
    {
      "name": "constant.numeric.eap",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "punctuation.terminator.eap",
      "match": ";"
    },
    {
      "name": "punctuation.section.parens.eap",
      "match": "\\(|\\)"
    },
    {
      "name": "variable.other.eap",
      "match": "\\b[A-Za-zΑ-Ωα-ω_][A-Za-zΑ-Ωα-ω0-9_]*\\b"
    }
  ]
}
