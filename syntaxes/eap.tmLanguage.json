{
  "scopeName": "source.eap",
  "patterns": [
    {
      "name": "comment.line.eap",
      "match": "//.*$"
    },
    {
      "name": "comment.block.eap",
      "begin": "/\\*",
      "end": "\\*/"
    },
    {
      "name": "string.quoted.double.eap",
      "begin": "\"",
      "end": "\"",
      "patterns": [{ "name": "constant.character.escape.eap", "match": "\\\\." }]
    },
    {
      "name": "keyword.control.eap",
      "match": "(?<![\\w\\d\\x7f-\\xff])(ΤΕΛΟΣ-ΔΙΑΔΙΚΑΣΙΑΣ|ΤΕΛΟΣ-ΣΥΝΑΡΤΗΣΗΣ|ΕΑΝ-ΤΕΛΟΣ|ΓΙΑ-ΤΕΛΟΣ|ΕΝΟΣΩ-ΤΕΛΟΣ|ΜΕ-ΒΗΜΑ|ΑΛΓΟΡΙΘΜΟΣ|ΣΤΑΘΕΡΕΣ|ΔΕΔΟΜΕΝΑ|ΑΡΧΗ|ΤΕΛΟΣ|ΕΑΝ|ΤΟΤΕ|ΑΛΛΙΩΣ|ΓΙΑ|ΑΠΟ|ΕΩΣ|ΜΕ|ΒΗΜΑ|ΕΠΑΝΑΛΑΒΕ|ΕΝΟΣΩ|ΜΕΧΡΙ|ΔΙΑΔΙΚΑΣΙΑ|ΣΥΝΑΡΤΗΣΗ|PRINT|READ|FUNCTION|PROCEDURE|BEGIN|END)(?![\\w\\d\\x7f-\\xff])"
    },
    {
      "name": "keyword.other.commands.eap",
      "match": "\\b(ΤΥΠΩΣΕ|ΔΙΑΒΑΣΕ|ΥΠΟΛΟΓΙΣΕ)\\b"
    },
    {
      "name": "constant.numeric.eap",
      "match": "\\b\\d+(\\.\\d+)?\\b"
    },
    {
      "name": "punctuation.terminator.eap",
      "match": ";"
    },
    {
      "name": "punctuation.section.parens.eap",
      "match": "\\(|\\)"
    },
    {
      "name": "variable.other.eap",
      "match": "\\b[A-Za-zΑ-Ωα-ω_][A-Za-zΑ-Ωα-ω0-9_]*\\b"
    }
  ]
}
