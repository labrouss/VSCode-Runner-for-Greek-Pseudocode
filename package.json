{
  "name": "eap-language",
  "displayName": "EAP Pseudocode",
  "description": "EAP pseudocode language support",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/labrouss/VSCode-Runner-for-Greek-Pseudocode.git"
  },
  "bugs": {
    "url": "https://github.com/labrouss/VSCode-Runner-for-Greek-Pseudocode/issues"
  },
  "homepage": "https://github.com/labrouss/VSCode-Runner-for-Greek-Pseudocode#readme",
  "publisher": "labrouss",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:eap",
    "onCommand:eap.run"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "eap",
        "aliases": [
          "EAP",
          "Pseudocode"
        ],
        "extensions": [
          ".eap"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "eap",
        "scopeName": "source.eap",
        "path": "./syntaxes/eap.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "eap.run",
        "title": "Run EAP Program"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/vscode": "^1.85.0",
    "typescript": "^5.9.3"
  }
}
