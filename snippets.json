{
  "ΤΥΠΩΣΕ": {
    "prefix": "ΤΥΠΩΣΕ",
    "body": [
      "ΤΥΠΩΣΕ(\"${1:κείμενο}\");$0"
    ],
    "description": "Εντολή εμφάνισης με εισαγωγικά και ερωτηματικό"
  },
  "ΔΙΑΒΑΣΕ": {
    "prefix": "ΔΙΑΒΑΣΕ",
    "body": [
      "ΔΙΑΒΑΣΕ(${1:μεταβλητή});$0"
    ],
    "description": "Εντολή εισόδου με ερωτηματικό"
  },
  "Επαναληπτική Δομή ΓΙΑ": {
    "prefix": "ΓΙΑ",
    "body": [
      "ΓΙΑ ${1:μεταβλητή} := ${2:τιμή1} ΕΩΣ ${3:τιμή2} ΕΠΑΝΑΛΑΒΕ",
      "\t${0:εντολές}",
      "ΓΙΑ-ΤΕΛΟΣ"
    ],
    "description": "Δομή ΓΙΑ ... ΑΠΟ ... ΕΩΣ ... ΕΠΑΝΑΛΑΒΕ"
  },
  "Επαναληπτική Δομή ΓΙΑ ΜΕ ΒΗΜΑ": {
    "prefix": "ΓΙΑ-ΒΗΜΑ",
    "body": [
      "ΓΙΑ ${1:μεταβλητή} := ${2:τιμή1} ΕΩΣ ${3:τιμή2} ΒΗΜΑ ${4:βήμα} ΕΠΑΝΑΛΑΒΕ",
      "\t${0:εντολές}",
      "ΓΙΑ-ΤΕΛΟΣ"
    ],
    "description": "Δομή ΓΙΑ ΜΕ ΒΗΜΑ"
  },
  "Επαναληπτική Δομή ΕΑΝ": {
    "prefix": "ΕΑΝ",
    "body": [
      "ΕΑΝ ${1:συνθήκη} ΤΟΤΕ",
      "\t${2:εντολές}",
      "ΑΛΛΙΩΣ",
      "\t${0:εντολές}",
      "ΕΑΝ-ΤΕΛΟΣ"
    ],
    "description": "Δομή ΕΑΝ ... ΤΟΤΕ ... ΑΛΛΙΩΣ"
  },
  "Επαναληπτική Δομή ΕΝΟΣΩ": {
    "prefix": "ΕΝΟΣΩ",
    "body": [
      "ΕΝΟΣΩ ${1:συνθήκη} ΕΠΑΝΑΛΑΒΕ",
      "\t${0:εντολές}",
      "ΕΝΟΣΩ-ΤΕΛΟΣ"
    ],
    "description": "Δομή ΕΝΟΣΩ ... ΕΠΑΝΑΛΑΒΕ"
  },
  "Σκελετός Αλγορίθμου": {
    "prefix": "ΑΛΓΟΡΙΘΜΟΣ",
    "body": [
      "ΑΛΓΟΡΙΘΜΟΣ ${1:Όνομα_Αλγορίθμου}",
      "ΑΡΧΗ",
      "\t$0",
      "ΤΕΛΟΣ // ${1:Όνομα_Αλγορίθμου}"
    ],
    "description": "Πλήρης δομή αλγορίθμου"
  },
  "Σκελετός Συνάρτησης": {
    "prefix": "ΣΥΝΑΡΤΗΣΗ",
    "body": [
      "ΣΥΝΑΡΤΗΣΗ ${1:Όνομα_Συνάρτησης} (${2:ΠΑΡΑΜΕΤΡΟΣ})",
      "ΔΙΕΠΑΦΗ",
      "ΕΙΣΟΔΟΣ",
      "${2:ΠΑΡΑΜΕΤΡΟΣ}",
      "ΕΞΟΔΟΣ",
      "${1:Όνομα_Συνάρτησης}",
      "ΔΕΔΟΜΕΝΑ",
      "ΑΡΧΗ",
      "\t$0",
      "ΤΕΛΟΣ-ΣΥΝΑΡΤΗΣΗΣ // ${1:Όνομα_Συνάρτησης}"
    ],
    "description": "Πλήρης δομή συνάρτησης"
  },
   "Σκελετός Διαδικασίας": {
    "prefix": "ΔΙΑΔΙΚΑΣΙΑ",
    "body": [
      "ΔΙΑΔΙΚΑΣΙΑ ${1:Όνομα_Διαδικασίας} (${2:ΠΑΡΑΜΕΤΡΟΣ})",
      "ΔΙΕΠΑΦΗ",
      "ΕΙΣΟΔΟΣ", 
      "${2:ΠΑΡΑΜΕΤΡΟΣ}",
      "ΕΞΟΔΟΣ",
      "${1:Όνομα_Διαδικασίας}",
      "ΔΕΔΟΜΕΝΑ",
      "ΑΡΧΗ",
      "\t$0",
      "ΤΕΛΟΣ-ΔΙΑΔΙΚΑΣΙΑΣ // ${1:Όνομα_Διαδικασίας}"
    ],
    "description": "Πλήρης δομή διαδικασίας"
  }

}
